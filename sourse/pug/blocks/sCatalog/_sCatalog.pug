mixin catalogItem(img,name,transmission,period,price,cost)
	+b.catalog-item
		+e.img-wrap
			.swiper.catalog-item-slider.catalog-item-slider--js
				.swiper-wrapper
					.swiper-slide
						+img(`img/catalog-${img}.jpg`)
					.swiper-slide
						+img(`img/catalog-${img}.jpg`)
					.swiper-slide
						+img(`img/catalog-${img}.jpg`)
					.swiper-slide
						+img(`img/catalog-${img}.jpg`)
					.swiper-slide
						+img(`img/catalog-${img}.jpg`)
				.swiper-pagination
		+e.text-wrap
			+e.about
				+e.name!= name
				+e.transmission!= transmission
			+e.total
				+e.period!= period
				+e.price!= price
			+e.more
				+e.cost!= cost
				+e.details(href="#") Подробнее
			+btn('Заказать в 1 клик').btn.btn-primary


